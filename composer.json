{
    "name": "jeremykenedy/laravel-users",
    "description": "Laravel Users Management CRUD Package",
    "keywords": ["laravel", "users", "management", "CRUD"],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "jeremykenedy",
            "email": "jeremykenedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3|^8.0",
        "laravellux/html": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "jeremykenedy\\laravelusers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jeremykenedy\\laravelusers\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.2",
        "laravel/tinker": "^2.9",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "laravel/laravel": "^8.0|^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "jeremykenedy\\laravelusers\\LaravelUsersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
